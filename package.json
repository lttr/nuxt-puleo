{
  "name": "@lttr/nuxt-puleo",
  "version": "0.0.8",
  "description": "Integrate Puleo into Nuxt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lttr/nuxt-puleo.git"
  },
  "license": "MIT",
  "author": "Lukas Trumm",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish --access public && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.1"
  },
  "devDependencies": {
    "@lttr/nuxt-config-eslint": "^0.3.0",
    "@lttr/nuxt-config-postcss": "^0.0.8",
    "@lttr/puleo": "^0.5.0",
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint-config": "^1.7.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.1",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.32.0",
    "nuxt": "^4.0.1",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  },
  "peerDependencies": {
    "@lttr/nuxt-config-postcss": "^0.0.8",
    "@lttr/puleo": "^0.5.0"
  }
}
